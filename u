    $('#newsletter').submit(function(e) {
        alert('am here');
        e.preventDefault();
        $('#preloader').show();
        var formData = new FormData(this);
    
        let email = $('#email').val();
        // let session = localStorage.setItem('id',email);
        
        formData.append('session', session);
        formData.append('email', email);
        
        $.ajax({
                type:'POST',
                url: "{{ route('subscribe-news')}}",
                data: formData,
                cache:false,
                contentType: false,
                processData: false,
                dataType: 'json',
            success: (data) => {
                this.reset();  // $('#thanks').show();
                // $('#preloader').hide();
                // alert('Subscribed');
                // window.location.href = "{{ route('about')}}";
              
            },
            error: function(data){
                // $('#preloader').hide();
                alert(data.responseJSON.errors.files[0]);
                console.log(data.responseJSON.errors);
            }
        });
    });
    let fname = $('#fname').val();



